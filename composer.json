{
    "name": "laravel/laravel-masbug-google-drive-demo",
    "description": "Masbug google drive plugin demo for Laravel Framework.",
    "keywords": ["framework", "laravel", "filesystem", "flysystem", "google", "google-drive", "google drive"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^9.10",
        "laravel/tinker": "^2.7",
        "masbug/flysystem-google-drive-ext": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        },
        "google/apiclient-services": [
            "Drive"
        ]
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "Google\\Task\\Composer::cleanup"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "Google\\Task\\Composer::cleanup"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
